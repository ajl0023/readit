{
  "extends": ["prettier", "plugin:node/recommended"],
  "plugins": ["prettier", "autofix"],
  "rules": {
    "prettier/prettier": ["warn", { "endOfLine": "auto" }],
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "autofix/no-debugger": "error",
    "object-shorthand": "off",
    "class-methods-use-this": "off"
  }
}
